{
  "name": "quiz-dangal",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://quizdangal.com",
  "scripts": {
    "dev": "vite",
    "dev:lan": "vite --host --port 5173",
    "prebuild": "node ./scripts/generate-maskable-icons.mjs && node ./scripts/generate-sitemap.mjs",
    "build": "vite build",
    "build:skip-sitemap": "cross-env SITEMAP_SKIP=1 npm run build",
    "postbuild": "node ./scripts/prerender-routes.mjs && echo Build complete",
    "build:secure": "vite build && node ./scripts/scan-dist-secrets.mjs dist",
    "analyze": "cross-env ANALYZE=true vite build && echo 'Open dist/stats.html'",
    "test": "vitest run --no-color",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "verify:api": "node ./scripts/verify-api.mjs",
    "scan:dist": "node ./scripts/scan-dist-secrets.mjs dist",
    "inspect:live": "node ./scripts/inspect-live.mjs",
    "sql:run": "node ./scripts/sql-run.mjs",
    "sql:repl": "node ./scripts/sql-repl.mjs",
    "inspect:schema": "node ./scripts/inspect-schema.mjs",
    "dev:db-tools": "node ./scripts/dev-tools.mjs",
    "generate:sitemap": "node ./scripts/generate-sitemap.mjs",
    "verify:seo": "node ./scripts/verify-seo.mjs",
    "preview": "vite preview",
    "preview:lan": "vite preview --host --port 5173",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint \"src/**/*.{js,jsx}\" --max-warnings=0",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "browserslist:update": "npx update-browserslist-db@latest",
    "check": "npm run lint && npm test",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@supabase/supabase-js": "^2.52.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.285.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.16.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.27.0",
    "@babel/traverse": "^7.27.0",
    "@babel/types": "^7.27.0",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.8.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "eslint": "^8.57.1",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "pg": "^8.16.3",
    "postcss": "^8.4.31",
    "prettier": "^3.7.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "sharp": "^0.34.5",
    "supabase": "^2.64.2",
    "tailwindcss": "^3.3.3",
    "terser": "^5.39.0",
    "vite": "^4.4.5",
    "vitest": "^3.2.4",
    "web-vitals": "^3.5.2"
  },
  "overrides": {
    "esbuild": "^0.25.11"
  }
}
